Задача 1 
a=int(input())
for b in range(1,11):
    print(f"{a}*{b} = {a*b}")

Задача 2 
a= int(input()) 
sum=0 
for i in range(a): 
 sum+=float(input()) 
print(sum)

Задача 3 
a = str(input())
while (a != "PRINT"):
    print("id"+a)
    a = str(input())

Задача 4 
print("Введите два числа:")
a = float(input())
b = float(input())
while (b != 0):
    print(a+b, a-b, a*b, a/b)
    a = float(input())
    b = float(input())
    print("На ноль делить нельзя")
